generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Presupuesto {
  id               String    @id @default(uuid())
  detalle          String
  montoAsignado    Float
  montoGastoReal   Float
  montoSaldo       Float
  soloPago         Boolean
  sobre            String
  fechaVencimiento DateTime?
}
